{
  "name": "enomotodev/laractive-admin",
  "description": " The administration framework for Laravel applications.",
  "keywords": ["framework", "laravel", "admin", "admin-ui"],
  "license": "MIT",
  "homepage": "https://enomotodev.hatenablog.com/",
  "support": {
    "issues": "https://github.com/enomotodev/laractive-admin/issues",
    "source": "https://github.com/enomotodev/laractive-admin"
  },
  "authors": [
    {
      "name": "enomotodev",
      "email": "enomoto.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "doctrine/dbal": "~2.3",
    "intervention/httpauth": "~2.0.3",
    "laravel/framework": "~5.5",
    "laravelcollective/html": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "laravel/laravel": "~5.5",
    "laravel/browser-kit-testing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Enomotodev\\LaractiveAdmin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enomotodev\\LaractiveAdmin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enomotodev\\LaractiveAdmin\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
